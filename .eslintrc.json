{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "plugins": [
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "semi": "off",
        "import/extensions": "off",
        "comma-dangle": "off",
        "no-unused-vars": "off",
        "consistent-return": "off",
        "no-plusplus": "off"
    }
}